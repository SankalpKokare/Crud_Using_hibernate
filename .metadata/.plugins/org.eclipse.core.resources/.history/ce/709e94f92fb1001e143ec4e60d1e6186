package entitity;

import java.sql.Date;

public class Emp {
	
	
	int empno;
	String ename;
	String job;
	int mgr;
	Date hiredate;
	double sal;
	Double comm;
	int deptno;
	
	public Emp() {
		super();
	}

	public Emp(int empno, String ename, String job, int mgr, Date hiredate, double sal, Double comm, int deptno) {
		super();
		this.empno = empno;
		this.ename = ename;
		this.job = job;
		this.mgr = mgr;
		this.hiredate = hiredate;
		this.sal = sal;
		this.comm = comm;
		this.deptno = deptno;
	}
	
	

}

